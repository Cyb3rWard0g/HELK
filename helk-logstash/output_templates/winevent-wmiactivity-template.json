{
  "index_patterns" : "logs-endpoint-winevent-wmiactivity-*",
  "settings" : {
    "index.refresh_interval": "5s"
  },
  "mappings":{
    "doc":{
      "dynamic_templates": [{
        "strings":{
          "match_mapping_type": "string",
          "mapping": {
            "type": "text",
            "norms": false,
            "fields": {
              "raw": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          }
        }
      }],
      "properties":{
        "@timestamp":{"type":"date"},
        "process_id":{"type":"integer"}
      }
    }
  }
}
